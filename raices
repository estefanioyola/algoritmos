// calculo de las raices reales.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include < iostream >
#include "math.h"
#include "conio.h"

using namespace std;

int main( )
{
float a,b,c,y,x1,x2;
	cin>>"Ingrese a">>a;
	cin>>"Ingrese a">>a;
	cin>>"Ingrese a">>a;

	y= pow(b,2)-4*a*c;
if (y>0)
{ x1 =(-b+ sqrt (y))/(2*a);
  x2 =(-b-sqrt (y))/(2*a);
  
  cout <<"x1 es"<<x1;
  cout<<"x2 es"<<x2;
}
else
{ cout<<("No son numeros reales");
}

getch ();
 
return 0;
  
}
